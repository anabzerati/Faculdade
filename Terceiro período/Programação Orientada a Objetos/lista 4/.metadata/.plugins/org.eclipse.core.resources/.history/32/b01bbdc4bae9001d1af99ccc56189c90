package formasGeomatricas;

public class Retangulo implements FormaGeomatrica{
	private double base;
	private double altura;
	
	public Retangulo(double base, double altura) {
		if(base < 0 || altura < 0) {
			throw new InputMismatchException("Valor negativo");
		}
		
		this.base = base;
		this.altura = altura;
	}
}
